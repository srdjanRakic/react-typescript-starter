{
  "name": "react-typescript-starter",
  "sideEffects": false,
  "version": "1.0.0",
  "description": "React TypeScript Starter",
  "author": "Srdjan Rakic",
  "license": "MIT",
  "engines": {
    "node": ">=6.9.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build"
  ],
  "keywords": [
    "react",
    "typescript",
    "webpack",
    "eslint",
    "starter"
  ],
  "main": "build/main.min.js",
  "browser": "build/main.min.js",
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "yarn start-message",
    "start": "concurrently -k -r -s first \"yarn open:src\" \"yarn lint:watch\"",
    "open:src": "cross-env NODE_ENV=development webpack-dev-server",
    "remove-build": "rimraf ./build",
    "clean-build": "yarn remove-build",
    "prebuild": "yarn lint && yarn clean-build",
    "build:staging": "cross-env NODE_ENV=development webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "format": "prettier --write 'packages/**/*.js'",
    "lint": "esw --ext .jsx,.js src/ --color",
    "lint:watch": "yarn lint --watch",
    "lint:fix": "eslint src/** --fix",
    "test": "jest src/**",
    "test:watch": "jest src/** --watch",
    "test:watchAll": "jest src/** --watchAll",
    "test:coverage": "jest --coverage",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "devDependencies": {
    "@types/jest": "^24.0.22",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.0",
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-sent": "^7.7.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.6.3",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@babel/register": "^7.7.0",
    "@babel/runtime": "^7.7.2",
    "@babel/runtime-corejs3": "^7.7.2",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "autoprefixer": "^9.7.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-css-import-to-string": "0.0.2",
    "babel-plugin-version-inline": "^1.0.0",
    "babel-upgrade": "^1.0.1",
    "chalk": "^3.0.0",
    "concurrently": "^5.0.0",
    "core-js": "3.4.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.19.1",
    "raf": "^3.4.1",
    "regenerator-runtime": "^0.13.3",
    "style-loader": "^1.0.0",
    "typescript": "^3.7.2",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^4.4.1"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill"
    ],
    "testRegex": "/src/.*\\.test\\.jsx?$",
    "modulePaths": [
      "<rootDir>/src/",
      "<rootDir>/src/__tests__"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test/",
      "<rootDir>/lib/",
      "<rootDir>/src/__tests__/testUtils.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test/",
      "<rootDir>/lib/",
      "<rootDir>/src/__tests__/testUtils.js"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ]
  }
}
